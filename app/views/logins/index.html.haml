%p#notice= notice

%table.table
  %thead
    %tr
      %th 账号
      %th Ip
      %th 状态
      %th{:colspan => "3"}
  %tbody
    - @logins.each do |login|
      %tr
        %td= login.ip
        %td= link_to '显示', edit_login_path(login)
        %td= link_to '删除', login, method: :delete, data: { confirm: 'Are you sure?' }


= paginate @logins
